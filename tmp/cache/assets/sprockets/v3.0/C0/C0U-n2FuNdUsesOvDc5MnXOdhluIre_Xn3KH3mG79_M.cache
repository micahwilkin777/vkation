<div class="clear20"></div>
<div class="">
	<strong class="title pull-left">Amenities</strong>
	<div class="col-md-2"></div>
	<div class="col-md-7">
		<span class="amenity-color" ng-if="hotel.facilities" ng-repeat="facility in hotel.facilities">{{facility}} | </span>
